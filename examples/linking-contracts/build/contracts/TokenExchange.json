{"fileName":"TokenExchange.sol","contractName":"TokenExchange","source":"pragma solidity ^0.5.0;\n\nimport \"zos-lib/contracts/Initializable.sol\";\nimport \"openzeppelin-eth/contracts/token/ERC20/IERC20.sol\";\nimport \"openzeppelin-eth/contracts/math/SafeMath.sol\";\n\ncontract TokenExchange is Initializable {\n  using SafeMath for uint256;\n\n  uint256 public rate;\n  IERC20 public token;\n\n  function initialize(uint256 _rate, IERC20 _token) public initializer {\n    rate = _rate;\n    token = _token;\n  }\n\n  function() external payable {\n    uint256 tokens = msg.value.mul(rate);\n    token.transfer(msg.sender, tokens);\n  }\n\n  // Added on subsequent version!\n  address public owner;\n\n  function withdraw() public {\n    require(msg.sender == owner);\n    msg.sender.transfer(address(this).balance);\n  }\n\n  function setOwner(address _owner) public {\n    require(owner == address(0));\n    owner = _owner;\n  }\n} \n","sourcePath":"contracts/TokenExchange.sol","sourceMap":"187:636:0:-;;;;8:9:-1;5:2;;;30:1;27;20:12;5:2;187:636:0;;;;;;;;;;","deployedSourceMap":"187:636:0:-;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;425:115;459:14;476:19;490:4;;;;476:9;:13;;:19;;;;:::i;:::-;459:36;;501:5;;;;;;;;;;;:14;;;516:10;528:6;501:34;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;8:9:-1;5:2;;;30:1;27;20:12;5:2;501:34:0;;;;8:9:-1;5:2;;;45:16;42:1;39;24:38;77:16;74:1;67:27;5:2;501:34:0;;;;;;;13:2:-1;8:3;5:11;2:2;;;29:1;26;19:12;2:2;501:34:0;;;;;;;;;;;;;;;;;425:115;;187:636;721:100;;8:9:-1;5:2;;;30:1;27;20:12;5:2;721:100:0;;;;;;13:2:-1;8:3;5:11;2:2;;;29:1;26;19:12;2:2;721:100:0;;;;;;;;;;;;;;;;;;;:::i;:::-;;262:19;;8:9:-1;5:2;;;30:1;27;20:12;5:2;262:19:0;;;:::i;:::-;;;;;;;;;;;;;;;;;;;603:114;;8:9:-1;5:2;;;30:1;27;20:12;5:2;603:114:0;;;:::i;:::-;;578:20;;8:9:-1;5:2;;;30:1;27;20:12;5:2;578:20:0;;;:::i;:::-;;;;;;;;;;;;;;;;;;;;;;;309:112;;8:9:-1;5:2;;;30:1;27;20:12;5:2;309:112:0;;;;;;13:2:-1;8:3;5:11;2:2;;;29:1;26;19:12;2:2;309:112:0;;;;;;;;;;;;;;;;;;;;;;;;;;;;:::i;:::-;;285:19;;8:9: